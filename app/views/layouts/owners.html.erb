<!DOCTYPE html>
<html>
  <head>
    <title>DogMa</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav id='nav'>
      <ul id='nav-ul'>
        <li><a href=<%= owner_path(current_owner()) %>>Your Account</a></li>
        <li><a href=<%= owners_path %>>Owners</a></li>
        <li><a href=<%= dogs_path %>>Dogs</a></li>
        <li><a href=<%= shelters_path %>>Shelters</a></li>
        <li><%= link_to 'Logout', owner_session_path(current_owner()), method: :delete %></li>
      </ul>
    </nav>

    <%= yield %>
  </body>
</html>
